{
  "extends": "../tsconfig.json",
  "include": ["./**/*.ts"],
  "compilerOptions": {
    "rootDir": "..",
    "noEmit": true,
    "plugins": [
      {
        "name": "@functionless/language-service"
      }
    ],
    "paths": {
      "@fnls": ["../lib/index"]
    },
    "sourceMap": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}
