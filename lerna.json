{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": [
    "packages/create-functionless",
    "packages/functionless",
    "packages/@functionless/*"
  ],
  "conventionalCommits": true,
  "commitHooks": false,
  "private": false,
  "createRelease": "github",
  "verifyAccess": false,
  "command": {
    "publish": {
      "npmClient": "npm",
      "allowBranch": ["main", "*"]
    }
  },
  "version": "0.26.0"
}
