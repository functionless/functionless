{
  "name": "@functionless/aws-constructs",
  "description": "Functionless, a TypeScript plugin and Construct library for the AWS CDK",
  "license": "Apache-2.0",
  "version": "0.27.0",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "repository": {
    "url": "github:functionless/functionless",
    "directory": "packages/@functionless/aws-constructs"
  },
  "bin": {
    "functionless": "./bin/functionless.js",
    "functionless.js": "bin/functionless.js"
  },
  "scripts": {
    "build": "ts-node ./scripts/sdk-gen.ts && tsc --build",
    "lint": "eslint --ext .ts,.tsx --fix --no-error-on-unmatched-pattern src test",
    "localstack": "./scripts/localstack",
    "test": "npm run test:fast && npm run test:runtime",
    "test:fast": "jest --all --testPathIgnorePatterns '(localstack|runtime)'",
    "test:runtime": "jest --all --updateSnapshot --testPathPattern '(localstack|runtime)' --no-cache",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p ./tsconfig.dev.json --noEmit",
    "dev": "tsc --build -w"
  },
  "devDependencies": {
    "@aws-cdk/aws-appsync-alpha": "2.44.0-alpha.0",
    "@aws-cdk/cloud-assembly-schema": "2.44.0",
    "@aws-cdk/cloudformation-diff": "2.44.0",
    "@aws-cdk/cx-api": "2.44.0",
    "@aws-sdk/client-dynamodb": "^3.180.0",
    "@functionless/jest": "^0.27.0",
    "@swc/jest": "^0.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.0.3",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^16",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "amplify-appsync-simulator": "^2.4.1",
    "aws-appsync": "4.0.0",
    "aws-cdk": "2.44.0",
    "aws-cdk-lib": "2.44.0",
    "aws-sdk": "^2",
    "axios": "^0.27.2",
    "cdk-assets": "2.44.0",
    "constructs": "10.0.0",
    "cross-fetch": "^3.1.5",
    "esbuild": "0.15.9",
    "eslint": "^8",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flatted": "^3.2.7",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.0.3",
    "jest-junit": "^13",
    "json-schema": "^0.4.0",
    "npm-check-updates": "^15",
    "prettier": "^2.7.1",
    "promptly": "^3.2.0",
    "proxy-agent": "^5.0.0",
    "standard-version": "^9",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "typesafe-dynamodb": "0.2.3",
    "typescript": "^4.8.3",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-appsync-alpha": "*",
    "aws-cdk-lib": "^2.44.0",
    "aws-sdk": "^2",
    "constructs": "^10.0.0",
    "esbuild": "^0.15.9",
    "typesafe-dynamodb": "^0.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@functionless/asl-graph": "^0.27.0",
    "@functionless/ast": "^0.27.0",
    "@functionless/ast-reflection": "^0.3.2",
    "@functionless/aws-lambda-constructs": "^0.27.0",
    "@functionless/error-code": "^0.27.0",
    "@functionless/nodejs-closure-serializer": "^0.1.2",
    "@functionless/util": "^0.27.0",
    "@functionless/vtl": "^0.27.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "1.2.245",
    "@swc/register": "^0.1.10",
    "@types/aws-lambda": "^8.10.106",
    "aws-lambda": "^1.0.7",
    "fs-extra": "^10.1.0",
    "minimatch": "^5.1.0",
    "source-map": "^0.7.4"
  },
  "jest": {
    "collectCoverage": false,
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/",
      "/lib"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@functionless/jest",
        {}
      ]
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.{tsx,jsx,ts,js,md,css}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
