{
  "name": "functionless",
  "description": "Functionless, a TypeScript plugin and Construct library for the AWS CDK",
  "repository": {
    "url": "github:functionless/functionless",
    "directory": "packages/core"
  },
  "bin": {
    "functionless": "./bin/functionless.js",
    "functionless.js": "bin/functionless.js"
  },
  "scripts": {
    "build": "scripts/run-task build",
    "build:website": "scripts/run-task build:website",
    "bump": "scripts/run-task bump",
    "clobber": "scripts/run-task clobber",
    "compile": "scripts/run-task compile",
    "eslint": "scripts/run-task eslint",
    "localstack": "scripts/run-task localstack",
    "package": "scripts/run-task package",
    "post-compile": "scripts/run-task post-compile",
    "post-upgrade": "scripts/run-task post-upgrade",
    "pre-compile": "scripts/run-task pre-compile",
    "release": "scripts/run-task release",
    "test": "scripts/run-task test",
    "test:app": "scripts/run-task test:app",
    "test:fast": "scripts/run-task test:fast",
    "test:runtime": "scripts/run-task test:runtime",
    "test:update": "scripts/run-task test:update",
    "test:watch": "scripts/run-task test:watch",
    "typecheck": "scripts/run-task typecheck",
    "unbump": "scripts/run-task unbump",
    "upgrade": "scripts/run-task upgrade",
    "watch": "scripts/run-task watch"
  },
  "devDependencies": {
    "@aws-cdk/aws-appsync-alpha": "2.43.1-alpha.0",
    "@aws-cdk/cloud-assembly-schema": "2.43.1",
    "@aws-cdk/cloudformation-diff": "2.43.1",
    "@aws-cdk/cx-api": "2.43.1",
    "@aws-sdk/client-dynamodb": "^3.180.0",
    "@swc/jest": "^0.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.0.3",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^14",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "amplify-appsync-simulator": "^2.4.1",
    "aws-appsync": "4.0.0",
    "aws-cdk": "2.43.1",
    "aws-cdk-lib": "2.43.1",
    "aws-sdk": "^2",
    "axios": "^0.27.2",
    "cdk-assets": "2.43.1",
    "constructs": "10.0.0",
    "cross-fetch": "^3.1.5",
    "esbuild": "0.15.9",
    "eslint": "^8",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flatted": "^3.2.7",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.0.3",
    "jest-junit": "^13",
    "json-schema": "^0.4.0",
    "npm-check-updates": "^15",
    "prettier": "^2.7.1",
    "projen": "^0.63.4",
    "promptly": "^3.2.0",
    "proxy-agent": "^5.0.0",
    "standard-version": "^9",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "typesafe-dynamodb": "0.1.5",
    "typescript": "4.8.2",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-appsync-alpha": "*",
    "aws-cdk-lib": "^2.43.1",
    "aws-sdk": "^2",
    "constructs": "^10.0.0",
    "esbuild": "^0.15.9",
    "typesafe-dynamodb": "^0.1.5",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@functionless/ast-reflection": "^0.3.1",
    "@functionless/nodejs-closure-serializer": "^0.1.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "1.2.245",
    "@swc/register": "^0.1.10",
    "@types/aws-lambda": "^8.10.106",
    "fs-extra": "^10.1.0",
    "minimatch": "^5.1.0",
    "source-map": "^0.7.4"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "version": "0.23.7-canary.1",
  "jest": {
    "collectCoverage": false,
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/",
      "/lib"
    ],
    "moduleNameMapper": {
      "^@fnls$": "<rootDir>/lib/index"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "./jest.js",
        {}
      ]
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ]
  },
  "types": "lib/index.d.ts",
  "resolutions": {
    "@types/responselike": "1.0.0",
    "got": "12.3.1"
  },
  "lint-staged": {
    "*.{tsx,jsx,ts,js,json,md,css}": [
      "eslint --fix"
    ]
  }
}
