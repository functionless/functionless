    "aws-cdk-lib": "^2.40.0",
    "chalk": "^5.0.1",
    "commander": "^9.4.0",
    "cross-spawn": "^7.0.3",
    "esbuild": "0.15.9",
    "fl-exp": "*",
    "functionless": "*",
    "mustache": "^4.2.0",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
